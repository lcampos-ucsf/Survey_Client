<%# Link to the "Previous" page
  - available local variables
    url:           url to the previous page
    current_page:  a page object for the currently displayed page
    num_pages:     total number of pages
    per_page:      number of items to fetch per page
    remote:        data-remote
-%>
<span class="prev">
  <!-- %= link_to_unless current_page.first?, raw(t 'views.pagination.previous'), url, :rel => 'prev', :remote => remote, :id => 'previousbtn',:onclick => 'formsubmit()' % -->
<% if url.index('invite') %>
	<%= link_to_unless current_page.first?, raw(t 'views.pagination.previous'), url, :rel => 'prev', :remote => remote, :class => "btn secondary medium", :onclick => "showmodaltransition()" %>
<% else %>
  <a onclick="formsubmit('<%= url %>','0', '<%= surveys_update_multiple_path %>');" class="btn secondary medium" id="previewbtn" >â€¹ Prev</a>

<% end %>
</span>
