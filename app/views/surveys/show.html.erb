	function keepvallist(element, sId){

		var txt = j$('#'+sId).val();
		var nlabel = element.id;

		if(txt.indexOf(nlabel) == -1){
			txt+= nlabel + ' ;';
			j$('#'+sId).val(txt);
		}else{
			txt = txt.replace(nlabel,'');
			j$('#'+sId).val(txt);
		}
	}
	

</script>

