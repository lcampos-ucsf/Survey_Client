	<input type="text" id="sac_data" class="sac_data" style="display:none;" autocomplete="off" />

	<a onclick="getstats(j$('#search_results'), j$('#sac_data').val(), '<%= stats_data_invite_index_path %>', '#search_results_header') " class="btn secondary" >Search</a>
