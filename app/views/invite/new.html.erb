<h1>Create</h1>

<%= form_for(@invite, :url => {:controller => "invite",:action => "create"}) do |f| %>
  
    <%= render 'fields', :f => f %>
    <div class="actions">
        <%= f.submit "Save", :class => 'btn primary customform' %>
        <a href="<%= invite_index_path %>" class="btn secondary " >Cancel</a>
    </div>
<% end %>