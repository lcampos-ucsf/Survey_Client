<%= widget_div do %>

<table>
	<thead>
		<tr>
			<td>Action</td>
			<td>Name</td>
			<td>Description</td>
			<td>Start Date</td>
			<td>End Date</td>
		</tr>
	</thead>
	<% @invites.each do |invite| %>

		<tbody>
			<tr>
				<td><a href="<%= invite.Id%>/show?page=1&sid=<%= invite.Survey__c%>" class="btn primary medium" > Start </a></td>
				<td><%= invite.Survey_Name__c %> </td>
				<td></td>
				<td><%= invite.Start_Date__c.strftime('%m/%d/%Y') %></td>
				<td><%= invite.End_Date__c.strftime('%m/%d/%Y') %></td>
			</tr>
		</tbody>

	<% end %>
</table>

<% end %>
